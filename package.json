{
  "name": "airbaker",
  "version": "1.0.0",
  "description": "Extended JavaScript Airtable Client with Redis-powered Sliding Window Rate Limiter and formula builder",
  "repository": "nadhifikbarw/airbaker",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src test",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src test -w",
    "release": "pnpm test && changelogen --release && git push --follow-tags",
    "test": "pnpm lint && vitest run --coverage"
  },
  "dependencies": {
    "airtable": "^0.12.2",
    "lodash-es": "^4.17.21",
    "p-retry": "^6.2.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.26",
    "@vitest/coverage-v8": "^1.3.1",
    "changelogen": "^0.5.5",
    "eslint": "^8.57.0",
    "eslint-config-unjs": "^0.2.1",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2",
    "unbuild": "^2.0.0",
    "vitest": "^1.3.1"
  },
  "packageManager": "pnpm@8.15.1"
}